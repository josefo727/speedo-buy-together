{
  "vendor": "speedoco",
  "name": "buy-together",
  "version": "0.0.1",
  "title": "Buy together",
  "description": "Buy together for speedo store",
  "builders": {
    "react": "3.x",
    "messages": "1.x",
    "docs": "0.x",
    "store": "0.x",
    "node": "7.x",
    "styles": "2.x"
  },
  "dependencies": {
    "vtex.product-context": "0.x",
    "vtex.store-resources": "0.x",
    "vtex.order-items": "0.x",
    "vtex.pixel-manager": "1.x",
    "vtex.flex-layout": "0.x",
    "vtex.rich-text": "0.x",
    "vtex.store-components": "3.x",
    "vtex.product-summary": "2.x",
    "vtex.css-handles": "1.x",
    "vtex.store": "2.x",
    "vtex.render-runtime": "8.x",
    "vtex.styleguide": "9.x",
    "vtex.slider-layout": "0.x"
  },
  "registries": [
    "smartcheckout"
  ],
  "policies": [
    {
      "name": "outbound-access",
      "attrs": {
        "host": "api.vtex.com",
        "path": "*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.vtexcommercestable.com.br",
        "path": "/api/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "portal.vtexcommercestable.com.br",
        "path": "/api/*"
      }
    },
    {
      "name": "ADMIN_DS"
    }
  ],
  "settingsSchema": {
    "title": "Speedoco API Keys",
    "type": "object",
    "properties": {
      "appKey": {
        "title": "VTEX App Key",
        "description": "The VTEX App Key for API authentication.",
        "type": "string"
      },
      "appToken": {
        "title": "VTEX App Token",
        "description": "The VTEX App Token for API authentication.",
        "type": "string"
      }
    }
  },
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
